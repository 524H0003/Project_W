{
	"debug.javascript.autoAttachFilter": "disabled",
	"editor.codeActionsOnSave": { "source.fixAll": "explicit" },
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.detectIndentation": false,
	"editor.foldingImportsByDefault": true,
	"editor.formatOnSave": true,
	"editor.insertSpaces": false,
	"editor.tabSize": 2,
	"editor.tokenColorCustomizations": { "comments": "", "textMateRules": [] },
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.patterns": {
		"*.entity.ts": "$(capture).model.ts, $(capture).graphql.ts",
		"*.ts": "$(capture).spec.ts, $(capture).spec.utils.ts",
		"package.json": "package-lock.json, pnpm*, .yarnrc*, yarn*, .eslint*, eslint*, .prettier*, prettier*, .editorconfig, LICENSE*, README.MD, nest-cli.json, procfile, .gitignore, codegen.ts, .env*",
		"tsconfig.json": "tsconfig.*.json",
		"jest.all.config.ts": "jest.*.config.ts"
	},
	"explorerExclude.backup": {},
	"files.eol": "\n",
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/dist": true,
		"**/node_modules": true,
		".adminjs": true,
		".devcontainer": true,
		".git": true,
		"LICENSE": true,
		"coverage": true,
		"documentation": true,
		"public": true,
		"reports": true,
		"secrets": true,
		"src/build.ts": true,
		"src/build": true,
		"src/types.ts": true,
		"types": true,
		"graphql": true
	},
	"jest.outputConfig": { "clearOnRun": "terminal" },
	"jest.runMode": {
		"coverage": true,
		"showInlineError": true,
		"type": "on-demand"
	},
	"testing.automaticallyOpenTestResults": "neverOpen",
	"testing.openTesting": "neverOpen",
	"todohighlight.keywords": [
		{
			"backgroundColor": "#00bfff",
			"color": "#4b3621",
			"isWholeLine": true,
