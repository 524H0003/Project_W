    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.5",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "daisyui": "^5.0.28",
    "eslint": "^9.25.1",
    "eslint-plugin-vue": "^10.0.1",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "~5.8.3",
