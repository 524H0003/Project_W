	"type": "commonjs",
	"scripts": {
		"buildfile": "ts-node src/build.ts",
		"doc": "compodoc src -p tsconfig.doc.json --disableSourceCode --hideGenerator",
		"prebuild": "npm i && npm run buildfile && npm run gqlgen && npm run codegen",
		"build": "cd ./mail && npm run build && cd .. && nest build",
		"format": "replace '{\\n' '{' . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" --silent && replace '\\[\\n' '[' . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" --silent && npm run prettier && npm run prettier",
		"lint": "eslint --fix ./src",
		"prettier": "prettier --write \"./**/*.ts\" \"./**/*.js\" \"./**/*.json\" \"./**/*.*rc\" --log-level silent",
		"start": "nest start",
		"start:debug": "npm run buildfile && nest start --debug --watch",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/src/main",
		"pretest": "npm run buildfile",
		"gqlgen": " gqlg --schemaFilePath ./src/schema.gql --destDirPath ./graphql/operations --ext graphql",
		"test": "jest --config=jest.all.config.ts",
		"test:controller": "jest --config=jest.controller.config.ts",
		"test:resolver": "jest --config=jest.resolver.config.ts",
		"test:service": "jest --config=jest.service.config.ts",
		"test:debug": "jest --debug",
		"types": "",
		"old_types": "tsc --project types/tsconfig.json && dts-bundle-generator -o types/index.d.ts src/types.ts --verbose",
		"codegen": "graphql-codegen --config codegen.ts"
	},
