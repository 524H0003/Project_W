	"scripts": {
		"buildfile": "ts-node src/build.ts",
		"doc": "compodoc src -p tsconfig.doc.json --disableSourceCode --hideGenerator -d ./src/app/docs",
		"mail": "cd ./mail && npm run build && rimraf node_modules && cd ..",
		"page": "npm run types && cd ./page && npm run build && rimraf node_modules && cd ..",
		"format": "replace '{\\n' '{' . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" && replace '\\[\\n' '[' . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" && replace '\\)\\n' ')' . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" && npm run prettier && npm run prettier",
		"lint": "eslint --fix ./src",
		"prettier": "prettier --write \"./**/*.ts\" \"./**/*.js\" \"./**/*.json\"",
		"gqlgen": " gqlg --schemaFilePath ./src/schema.gql --destDirPath ./graphql/operations --ext graphql",
		"types": "npm run buildfile && tsc --project types/tsconfig.json && dts-bundle-generator -o types/index.d.ts src/build/types.ts --verbose",
		"codegen": "npm run gqlgen && graphql-codegen --config codegen.ts",
		"prestart:ci": "npm run build && pnpm i --prod",
		"start": "nest start",
		"start:debug": "npm run buildfile && nest start --debug --watch",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/src/main",
		"start:ci": "start-server-and-test start:web :3000 test:web",
		"start:web": "npm run start:prod -- --test-email",
		"prebuild:dev": "pnpm i && (npm run buildfile & (npm run gqlgen && npm run codegen))",
		"prebuild": "pnpm i && (npm-run-all --parallel mail page codegen doc)",
		"build:dev": "nest build",
		"build": "nest build",
		"test": "jest --config=jest.all.config.ts",
		"test:web": "cypress install && cypress run",
		"test:requesters": "jest --config=jest.requesters.config.ts",
		"test:service": "jest --config=jest.service.config.ts",
		"test:etc": "jest --config=jest.etc.config.ts",
		"test:debug": "jest --debug",
		"migration:generate": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/app/typeorm/typeorm.config.ts ./src/app/typeorm/migrations/migration",
		"migration:create": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create ./src/app/typeorm/migrations/migration",
		"migration:run": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/app/typeorm/typeorm.config.ts",
		"migration:revert": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/app/typeorm/typeorm.config.ts"
	},
