	"type": "commonjs",
	"scripts": {
		"buildfile": "ts-node src/build.ts",
		"doc": "compodoc src -p tsconfig.doc.json --disableSourceCode --hideGenerator",
		"mail": "cd ./mail && npm run build && cd ..",
		"page": "npm run types && cd ./page && npm run build && cd ..",
		"format": "replace '{\\n' '{' . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" --silent && replace '\\[\\n' '[' . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" --silent && replace '\\)\\n' ')' . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" --silent && npm run prettier && npm run prettier",
		"lint": "eslint --fix ./src",
		"prettier": "prettier --write \"./**/*.ts\" \"./**/*.js\" \"./**/*.json\" \"./**/*.*rc\" --log-level silent",
		"gqlgen": " gqlg --schemaFilePath ./src/schema.gql --destDirPath ./graphql/operations --ext graphql",
		"types": "npm run buildfile && tsc --project types/tsconfig.json && dts-bundle-generator -o types/index.d.ts src/types.ts --verbose",
		"codegen": "npm run gqlgen && graphql-codegen --config codegen.ts",
		"prestart:ci": "npm run build && cypress install",
		"start": "nest start",
		"start:debug": "npm run buildfile && nest start --debug --watch",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/src/main",
		"start:ci": "start-server-and-test start:prod :3000 test:web",
		"prebuild:dev": "pnpm i && (npm run buildfile & (npm run gqlgen && npm run codegen))",
		"prebuild": "pnpm i && (npm-run-all --parallel mail page codegen)",
		"build:dev": "nest build",
		"build": "nest build",
		"test": "jest --config=jest.all.config.ts",
		"test:web": "cypress run",
		"test:controller": "jest --config=jest.controller.config.ts",
		"test:resolver": "jest --config=jest.resolver.config.ts",
		"test:service": "jest --config=jest.service.config.ts",
		"test:debug": "jest --debug"
	},
