	"type": "commonjs",
	"scripts": {
		"buildfile": "ts-node src/build.ts",
		"doc": "compodoc src -p tsconfig.doc.json --disableSourceCode --hideGenerator",
		"prebuild": "npm i && npm run buildfile && npm run gqlgen && npm run codegen",
		"build": "cd ./mail && npm run build && cd .. && nest build",
		"format": "replace '{\n' '{' . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" --silent && replace \"\\[\n\" \"[\" . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" --silent && prettier --write \"./**/*.ts\" \"./**/*.js*\" \"./**/*.*rc\" --log-level silent",
		"lint": "eslint --fix  --ext .ts .",
		"safe": "generate-env-example",
		"start": "nest start",
		"start:debug": "npm run buildfile && nest start --debug --watch",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/main",
		"pretest": "npm run buildfile",
		"gqlgen": " gqlg --schemaFilePath ./src/schema.gql --destDirPath ./graphql/operations --ext graphql",
		"test": "jest --config=jest.all.config.ts",
		"test:e2e": "jest --config=jest.e2e.config.ts",
		"test:unit": "jest --config=jest.unit.config.ts",
		"test:debug": "jest --debug",
		"types": "tsc --project types/tsconfig.json && dts-bundle-generator -o types/index.d.ts src/types.ts",
		"triathlon": "npm run build && npm run test:unit && npm run test:e2e && npm run format && npm run lint",
		"codegen": "graphql-codegen --config codegen.ts"
	},
